{
	/* Coloring
	 *
	 * General
	 * 	- Border Coloring: #A3A3A3
	 * 	- Color for boxes (textboxes) #D4D4D6 (or #B0B0B5)
	 * 	- placeholder text for light box #A5A5A5
	 * 	- placeholder text for dark box #1E1E1E
	 * 	- Dark text #16131B
	 *
	 * 	- Text Editor background: #2A2B35
	 *	- Text Editor selected line: #353747
	 *	- Text Editor highlighting lines: #475155
	 *
	 * Menu Coloring
	 * 	- Activity/Side bar color: #BDBDC3 (or #BEBEC4)
	 *	- Foreground color: #16131B
	 *	- Sidebar highlight: #D2D2D2 (or #F6F6F6)
	 *	- Selection Focus: #4684EB
	 * 	- Selection Focus 2: #B8B7BF
	 *	- Selection Focus 3: #2460C4
	 *
	 * Tab Bar
	 *	- Selected Tab bar #D2D2D2
	 *	- Unselected tab bar #AAAAAA
	 *	- Tab Bar panel #ECECEC
	 *
	 * Terminal
	 *	- Text
	 */


	"name": "Civic",
	"type": "dark",
	"colors": {
		
		// General
		"menu.selectionBorder": "#A3A3A3",
		"focusBorder": "#A3A3A3",
		"editorGroupHeader.tabsBackground": "#ECECEC",
		
		// Button
		"button.background": "#4684EB",
		"button.hoverBackground": "#2460C4",

		// Editor
		"editor.background": "#2A2B35",
		"editorCursor.foreground": "#ffffff",
		"editor.lineHighlightBackground": "#353747",
		"editor.lineHighlightBorder": "#353747",


		// Activity/Side bar
		"activityBar.background": "#BDBDC3",
		"activityBar.foreground": "#16131B",
		"activityBar.border": "#A3A3A3",
		"badge.background": "#4684EB",
		"badge.foreground": "#ffffff",

		// Activity/Side bar git text
		"sideBar.background": "#BEBEC4",
		"sideBar.foreground": "#16131B",
		"sideBar.border": "#ECECEC",

		"list.activeSelectionBackground": "#4684EB",
		"list.inactiveSelectionBackground": "#D2D2D2",
		"list.hoverBackground": "#D2D2D2",
		"list.inactiveFocusBackground": "#D2D2D2",
		"list.focusBackground": "#D2D2D2",

		"input.background": "#B0B0B5",
		"input.foreground": "#16131B",
		"input.placeholderForeground": "#16131B",
		"input.border": "#A3A3A3",

		// Tab Bar
		"tab.activeBackground": "#D2D2D2",
		"tab.activeForeground": "#16131B",
		"tab.inactiveBackground": "#AAAAAA",
		"tab.inactiveForeground": "#16131B",
		"tab.border": "#A5A5A5",
		"tab.hoverBackground": "#ECECEC",

		// Integrated Terminal
		"panel.background": "#2A2B35",
		"panelTitle.activeForeground": "#A7A3B9",
		"panel.border": "#A3A3A3",
		"panelTitle.activeBorder": "#2460C4",
		"panelTitle.inactiveForeground": "#A7A3B9",
		"dropdown.background": "#1E1E1E",

		// Status Bar
		"statusBar.background": "#4684EB",
		"statusBar.noFolderBackground": "#2460C4",
		"statusBar.border": "#A3A3A3",

		// Scroll Bar
		"scrollbarSlider.activeBackground": "#BDBDC3",

		// Git Colors
		"gitDecoration.addedResourceForeground": "#078d00",
		"gitDecoration.conflictingResourceForeground": "#e29300",
		"gitDecoration.untrackedResourceForeground": "#4e4e4c",
		"gitDecoration.deletedResourceForeground": "#9b000a",
		"gitDecoration.ignoredResourceForeground": "#5124E3",
		"gitDecoration.modifiedResourceForeground": "#003da0",
		"gitDecoration.submoduleResourceForeground": "#af5500",
	},
